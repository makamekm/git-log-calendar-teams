{
  "name": "git-log-calendar-teams",
  "description": "The tool generates git calendar logs depending on teams' activities",
  "version": "1.0.4",
  "private": false,
  "author": "Maxim Karpov <makame@mail.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/makamekm/git-log-calendar-teams.git"
  },
  "keywords": [
    "git",
    "log",
    "calendar",
    "teams",
    "cli"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\"",
    "build": "nexe -t win32-x86-10.13.0 ./cli.js -o dist/git-log.exe"
  },
  "bin": {
    "git-log-calendar-teams": "./cli.js"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "color-generator": "^0.1.0",
    "d3": "^5.15.0",
    "d3-node": "^2.2.1",
    "fs-extra": "^8.1.0",
    "spawnback": "^1.0.0",
    "yaml": "^1.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nexe": "^3.3.2",
    "prettier": "^1.19.1"
  }
}
