{
  "name": "git-log-calendar-teams",
  "description": "The tool generates git calendar logs depending on teams' activities",
  "version": "1.0.0",
  "private": false,
  "author": "Maxim Karpov <makame@mail.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/makamekm/git-log-calendar-teams.git"
  },
  "keywords": [
    "git",
    "log",
    "calendar",
    "teams",
    "cli"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\"",
    "is-master": "git-branch-is master",
    "release-it:patch": "release-it patch -n",
    "release-it:minor": "release-it minor -n",
    "release-it:major": "release-it major -n",
    "release:patch": "run-s is-master lint release-it:patch",
    "release:minor": "run-s is-master lint release-it:minor",
    "release:major": "run-s is-master lint release-it:major"
  },
  "bin": {
    "git-log-calendar-teams": "./cli.js"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "d3": "^5.15.0",
    "d3-node": "^2.2.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "spawnback": "^1.0.0",
    "yaml": "^1.8.2"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "git-branch-is": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "release-it": "^12.6.0"
  }
}
